DFRobot_OxygenSensor Oxygen;
void setup() 
{
  Serial.begin(9600);
  pinMode(LED, OUTPUT);
   pinMode(7,INPUT);
pinMode(Relaypin1, OUTPUT);
pinMode(Relaypin2, OUTPUT);
  while(!Oxygen.begin(Oxygen_IICAddress)) {
    Serial.println("I2c device number error !");
    delay(1000);
  }
  Serial.println("I2c connect success !");
}

