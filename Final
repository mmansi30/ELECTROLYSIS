#include "DFRobot_OxygenSensor.h"

#define COLLECT_NUMBER    10             // collect number, the collection range is 1-100.
#define Oxygen_IICAddress ADDRESS_2

#define LED 12

int Relaypin1= 3;
int Relaypin2= 2;
int Liquid_level=0;

//Pressure Sensor 100PSI

float voltage;
float pressure_pascal;
